    "tap-dot": "^1.0.0"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
